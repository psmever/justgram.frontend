  "scripts": {
    "start": "BROWSER=firefox && unset REACT_APP_ENV && unset REACT_APP_API_URL && cp config/development.env .env && PORT=4000 react-scripts start --reset-cache",
    "start:prod": "BROWSER=firefox && unset REACT_APP_ENV && unset REACT_APP_API_URL && cp config/production.env .env && PORT=4000 react-scripts start",
    "build": "unset REACT_APP_ENV && unset REACT_APP_API_URL && cp config/development.env .env && react-scripts build",
    "build:prod": "unset REACT_APP_ENV && unset REACT_APP_API_URL && cp config/production.env .env && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build:prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
